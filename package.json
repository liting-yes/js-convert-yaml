{
  "name": "js-convert-yaml",
  "description": "Convert javascript and yaml to each other",
  "author": "liting-yes <luz.liting@gmail.com> (https://liting.ink/)",
  "license": "MIT",
  "homepage": "https://github.com/liting-yes/js-convert-yaml#readme",
  "bugs": {
    "url": "https://github.com/liting-yes/js-convert-yaml/issues"
  },
  "keywords": [
    "javascript",
    "yaml",
    "convert"
  ],
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . && stylelint \"**/*.{css,scss}\"",
    "format": "eslint --fix . && stylelint --fix \"**/*.{css,scss}\"",
    "commit": "git add . && git-cz",
    "prepare": "npx simple-git-hooks"
  },
  "dependencies": {
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.2",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/cz-commitlint": "^17.7.1",
    "@iconify/json": "^2.2.101",
    "@nuxt/image-edge": "1.0.0-28101255.bece998",
    "@nuxthq/ui": "^2.7.0",
    "@nuxtjs/color-mode": "^3.3.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/fontaine": "^0.4.1",
    "@nuxtjs/stylelint-module": "^5.1.0",
    "@types/node": "^18",
    "@unocss/eslint-config": "^0.55.0",
    "@unocss/nuxt": "^0.52.7",
    "@unocss/reset": "^0.55.0",
    "@vueuse/core": "^10.3.0",
    "@vueuse/nuxt": "^10.3.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.46.0",
    "inquirer": "8",
    "lint-staged": "^13.3.0",
    "monaco-editor": "^0.41.0",
    "nuxt": "^3.6.5",
    "nuxt-lodash": "^2.5.0",
    "sass": "^1.65.1",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.10.2",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "typescript": "^5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{ts,vue,json,md}": "eslint --fix"
  }
}
